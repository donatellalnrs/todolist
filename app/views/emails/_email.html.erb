<div id="<%=email.id%>" class="card">
<%= check_box_tag :name, true, false, data: { remote: true, url: edit_email_path(email.id), params: { object: email.object, body: email.body }.to_param, method: "put" }%>
  <% if email.read == true %>
    <%= link_to "#{email.object}", email_path(email.id), remote:true %>
  <% else %>
    <strong><%= link_to "#{email.object}", email_path(email.id), remote:true %></strong>
  <% end %>
</div>
